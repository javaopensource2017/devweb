<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
	<name>framework</name>
	
	<component>
		<component-type>com.is.faces.rangeslider.Slider</component-type>
		<component-class>com.is.faces.rangeslider.Slider</component-class>
	</component>
	
	<component>
		<component-type>com.is.faces.tooltip.ToolTip</component-type>
		<component-class>com.is.faces.tooltip.ToolTip</component-class>
	</component>
	
	<component>
		<component-type>com.is.faces.inputfile.FileUploader</component-type>
		<component-class>com.is.faces.inputfile.FileUploader</component-class>
	</component>
	
	<component>
		<component-type>com.is.faces.calender.CalenderMy</component-type>
		<component-class>com.is.faces.calender.CalenderMy</component-class>
	</component>
	<component>
		<component-type>com.is.faces.commandbatton.MyCommandButton</component-type>
		<component-class>com.is.faces.commandbatton.MyCommandButton</component-class>
	</component>
	
	<render-kit>
		<renderer>
			<component-family>com.is.faces.rangeslider</component-family>
			<renderer-type>com.is.faces.rangeslider.SliderRenderer</renderer-type>
			<renderer-class>com.is.faces.rangeslider.SliderRenderer</renderer-class>
		</renderer>
		
		<renderer>
			<component-family>com.is.faces.tooltip</component-family>
			<renderer-type>com.is.faces.tooltip.ToolTipRenderer</renderer-type>
			<renderer-class>com.is.faces.tooltip.ToolTipRenderer</renderer-class>
		</renderer>
		
		<renderer>
			<component-family>com.is.faces.inputfile</component-family>
			<renderer-type>com.is.faces.inputfile.FileUploaderRenderer</renderer-type>
			<renderer-class>com.is.faces.inputfile.FileUploaderRenderer</renderer-class>
		</renderer>
		
			<renderer>
			<component-family>com.is.faces.calender</component-family>
			<renderer-type>com.is.faces.calender.CalenderMyRenderer</renderer-type>
			<renderer-class>com.is.faces.calender.CalenderMyRenderer</renderer-class>
		</renderer>
		
		<renderer>
			<component-family>com.is.faces.calender</component-family>
			<renderer-type>com.is.faces.commandbatton.MyCommandButtonRenderer</renderer-type>
			<renderer-class>com.is.faces.commandbatton.MyCommandButtonRenderer</renderer-class>
		</renderer>
		
		
		
		
	</render-kit>	
</faces-config>
